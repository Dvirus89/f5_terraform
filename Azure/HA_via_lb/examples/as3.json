{
  "class": "AS3",
  "action": "deploy",
  "persist": true,
  "declaration": {
    "class": "ADC",
    "schemaVersion": "3.13.0",
    "id": "123abc",
    "label": "Sample 1",
    "remark": "An HTTPS sample application",
    "controls": {
      "trace": true
    },
    "Sample_01": {
      "class": "Tenant",
      "A1": {
        "class": "Application",
        "template": "https",
        "serviceMain": {
          "class": "Service_HTTPS",
          "virtualPort": 8443,
          "virtualAddresses": [
            "0.0.0.0"
          ],
          "pool": "web_pool",
          "serverTLS": "webtls"
        },
        "web_pool": {
          "class": "Pool",
          "monitors": [
            "tcp"
          ],
          "members": [{
            "servicePort": 80,
            "addressDiscovery": "fqdn",
            "autoPopulate": true,
            "hostname": "httpbin.org"
          }]
        },
        "webtls": {
          "class": "TLS_Server",
          "certificates": [{
            "certificate": "webcert"
          }]
        }
      }
    }
  }
}
